<script src="https://framework-gb.cdn.gob.mx/assets/scripts/jquery-ui-datepicker.js"></script>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script type="text/javascript">
        const urlParams = new URLSearchParams(window.location.search);
        const idPredio = urlParams.get('id');
        if (idPredio != null) {
            getPredio(idPredio);
        }
        $(document).ready(function () {
            $(".breadcrumb").html("<li><i class='fa fa-home'></i></li><li><a href='/Levantamiento'>Lista de predios</a></li>");
            $('#fecha_recorrido').datepicker({
                dateFormat: 'yy/mm/dd',
                changeYear: true
            });
            $("#formInsertFormatoHomologacion").submit(function (e) {
                $("[name=DistDrenaje]").val($("#distanciaDrenaje" + $("[name=CveDrenaje]:checked").val()).val());
                $("[name=DistAguaPotable]").val($("#distanciaAguaPotable" + $("[name=CveAguaPotable]:checked").val()).val());
                $("[name=DistanciaVialidadConexion]").val($("#vialidad_distancia_" + $("[name=TipoVialidadConexion]:checked").val()).val());
                $("[name=MaterialesVialidadConexion]").val($("#vialidad_materiales_" + $("[name=TipoVialidadConexion]:checked").val()).val());
                $("[name=DistanciaAccesoPredio]").val($("#acceso_distancia_" + $("[name=TipoAccesoPredio]:checked").val()).val());
                $("[name=MaterialesAccesoPredio]").val($("#acceso_materiales_" + $("[name=TipoAccesoPredio]:checked").val()).val());
                var topografiaTerreno = '';
                $("input[name=CveTopografiaTerreno]").each(function () {
                    if (this.checked) {
                        topografiaTerreno = topografiaTerreno + $(this).val() + ",";
                    }
                });
                $("[name=TopografiaTerreno]").val(topografiaTerreno.slice(0,-1));
                var hidrografia = '';
                $("input[name=CveHidrografia]").each(function () {
                    if (this.checked) {
                        hidrografia = hidrografia + $(this).val() + ",";
                    }
                });
                $("[name=Hidrografia]").val(hidrografia.slice(0, -1));
                var restriccionesfed = '';
                $("input[name=CveRestriccionesFederales]").each(function () {
                    if (this.checked) {
                        restriccionesfed = restriccionesfed + $(this).val() + ",";
                    }
                });
                $("[name=RestriccionesFederales]").val(restriccionesfed.slice(0, -1));
                var caracteristicasSuelo = '';
                $("input[name=CveCaracteristicasSuelo]").each(function () {
                    if (this.checked) {
                        caracteristicasSuelo = caracteristicasSuelo + $(this).val() + ",";
                    }
                });
                $("[name=CaracteristicasSuelo]").val(caracteristicasSuelo.slice(0, -1));
                var otrasObservaciones = '';
                $("input[name=CveOtrasObservaciones]").each(function () {
                    if (this.checked) {
                        otrasObservaciones = otrasObservaciones + $(this).val() + ",";
                    }
                });
                $("[name=OtrasObservaciones]").val(otrasObservaciones.slice(0,-1));
                //return false;
            });
        });
        function getMunicipios() {
            $.ajax({
                url: "@Url.Action("GetMunicipios", "Levantamiento")", // Url
                data: {
                    // Datos / Parámetros
                    cveedo: $("#slctEstados option:selected").val()
                },
                type: "post"  // Verbo HTTP
            }).done(function (result) {
                $("#slctMunicipios option:gt(0)").remove();
                $("#slctLocalidades option:gt(0)").remove();
                result.data.forEach(function (item, index, arr) {
                    $("#slctMunicipios").append("<option value = "+ item.clave +">"+ item.descripcion +"</option>");
                });
            });
        }
        function getLocalidades() {
            $.ajax({
                url: "@Url.Action("GetLocalidades", "Levantamiento")", // Url
                data: {
                    // Datos / Parámetros
                    cveedo: $("#slctEstados option:selected").val(),
                    cvemun: $("#slctMunicipios option:selected").val()
                },
                type: "post"  // Verbo HTTP
            }).done(function (result) {
                $("#slctLocalidades option:gt(0)").remove();
                result.data.forEach(function (item, index, arr) {
                    $("#slctLocalidades").append("<option value = "+ item.clave +">"+ item.descripcion +"</option>");
                });
            });
        }
        function getPredio(id) {
            var data = new FormData();
            data.append("idPredio", id);
            $.ajax({
                type: "post",
                url: "@Url.Action("GetFormatoLevantamiento", "Levantamiento")", // Url
                data: data,
                processData: false,
                contentType: false,
                success: function (result) {
                    $('#spinner').hide();
                    if (result.id != null) {
                        $("[name='Id']").val(result.id);
                        $("#fecha_recorrido").val(result.fechaRecorrido);
                        $("#domicilio").val(result.domicilio);
                        $("#c_norte").val(result.coordN);
                        $("#c_oeste").val(result.coordO);
                        $("#slctEstados option[value=" + result.cveEstado + "]").prop("selected", true);
                        $("#slctEstados").trigger("change");
                        setTimeout(function () { $("#slctMunicipios option[value=" + result.cveMunicipio + "]").prop("selected", true); }, 1000);
                        setTimeout(function () { $("#slctMunicipios").trigger("change"); }, 1000);
                        setTimeout(function () { $("#slctLocalidades option[value=" + result.cveLocalidad + "]").prop("selected", true); }, 2000);
                        $("[name='CveTipoPredio'][value='" + result.cveTipoPredio + "']").prop("checked", true);
                        $("[name='CveLocalizacion'][value='" + result.cveLocalizacion + "']").prop("checked", true);
                        $("[name='CveAcceso'][value='" + result.cveAcceso + "']").prop("checked", true);
                        $("[name='CveOtrasCaracteristicas'][value='" + result.cveOtrasCaracteristicas + "']").prop("checked", true);
                        $("[name='CaracPredioOtro']").val(result.caracPredioOtro);
                        $("[name='CveElectricidad'][value='" + result.cveElectricidad + "']").prop("checked", true);
                        $("[name='DistElectricidad']").val(result.distElectricidad);
                        $("[name='CveDrenaje'][value='" + result.cveDrenaje + "']").prop("checked", true);
                        $("#distanciaDrenaje" + result.cveDrenaje).val(result.distDrenaje);
                        $("[name='CveAguaPotable'][value='" + result.cveAguaPotable + "']").prop("checked", true);
                        $("#distanciaAguaPotable" + result.cveAguaPotable).val(result.distAguaPotable);
                        $("#nivelesConstruidos").val(result.nivelesConstruidos);
                        $("#materialesNaturales").val(result.materialesNaturales);
                        $("#materialesIndustrializados").val(result.materialesIndustrializados);
                        $("[name='TieneConexionVialidad'][value='" + result.tieneConexionVialidad + "']").prop("checked", true);
                        if (result.tieneConexionVialidad == 1) {
                            $("[name='TipoVialidadConexion'][value='" + result.tipoVialidadConexion + "']").prop("checked", true);
                            $("#vialidad_distancia_" + result.tipoVialidadConexion).val(result.distanciaVialidadConexion);
                            $("#condicionesVialidadConexion" + result.tipoVialidadConexion +"[value='" + result.condicionesVialidadConexion +"']").prop("checked", true);
                            $("#vialidad_materiales_" + result.tipoVialidadConexion).val(result.materialesVialidadConexion);
                        }
                        $("[name='TieneAccesoPredio'][value='" + result.tieneAccesoPredio + "']").prop("checked", true);
                        if (result.tieneAccesoPredio == 1) {
                            $("[name='TipoAccesoPredio'][value='" + result.tipoAccesoPredio + "']").prop("checked", true);
                            $("#acceso_distancia_" + result.tipoAccesoPredio).val(result.distanciaAccesoPredio);
                            $("#condicionesAccesoPredio" + result.tipoAccesoPredio +"[value='" + result.condicionesAccesoPredio +"']").prop("checked", true);
                            $("#acceso_materiales_" + result.tipoAccesoPredio).val(result.materialesAccesoPredio);
                        }
                        $("#transporte_inmediato_distancia").val(result.distanciaTranPubAccesoInmediato);
                        $("[name='CondicionesTranPubAccesoInmediato'][value='" + result.condicionesTranPubAccesoInmediato + "']").prop("checked", true);
                        $("#transporte_comoda_distancia").val(result.distanciaTranPubComoda);
                        $("[name='CondicionesTranPubComoda'][value='" + result.condicionesTranPubComoda + "']").prop("checked", true);
                        $("#transporte_lejano_distancia").val(result.distanciaTranPubLejano);
                        $("[name='CondicionesTranPubLejano'][value='" + result.condicionesTranPubLejano + "']").prop("checked", true);
                        $("#estructura_parada_distancia").val(result.distanciaTranPubParada);
                        $("[name='CondicionesTranPubParada'][value='" + result.condicionesTranPubParada + "']").prop("checked", true);
                        $("#estructura_estacion_distancia").val(result.distanciaTranPubEstacionCetram);
                        $("[name='DistanciaTranPubEstacionCetram'][value='" + result.distanciaTranPubEstacionCetram + "']").prop("checked", true);
                        $("#estructura_sitio_distancia").val(result.distanciaTranPubSitioTaxis);
                        $("[name='CondicionesTranPubSitioTaxis'][value='" + result.condicionesTranPubSitioTaxis + "']").prop("checked", true);
                        $("[name='TranPubOtro']").val(result.tranPubOtro);
                        $("[name='DistanciaTranPubOtro']").val(result.distanciaTranPubOtro);
                        $("[name='CondicionesTranPubOtro'][value='" + result.condicionesTranPubOtro + "']").prop("checked", true);
                        $("[name='EqUrbEducacionNivel']").val(result.eqUrbEducacionNivel);
                        $("[name='EqUrbEducacionDistancia']").val(result.eqUrbEducacionDistancia);
                        $("[name='EqUrbSaludNivel']").val(result.eqUrbSaludNivel);
                        $("[name='EqUrbSaludDistancia']").val(result.eqUrbSaludDistancia);
                        $("[name='EqUrbRecreacionNivel']").val(result.eqUrbRecreacionNivel);
                        $("[name='EqUrbRecreacionDistancia']").val(result.eqUrbRecreacionDistancia);
                        $("[name='EqUrbCulturaNivel']").val(result.eqUrbCulturaNivel);
                        $("[name='EqUrbCulturaDistancia']").val(result.eqUrbCulturaDistancia);
                        $("[name='EqUrbServUrbanosNivel']").val(result.eqUrbServUrbanosNivel);
                        $("[name='EqUrbServUrbanosDistancia']").val(result.eqUrbServUrbanosDistancia);
                        $("[name='EqUrbAsistenciaSocialNivel']").val(result.eqUrbAsistenciaSocialNivel);
                        $("[name='EqUrbAsistenciaSocialDistancia']").val(result.eqUrbAsistenciaSocialDistancia);
                        $("[name='EqUrbOtro']").val(result.eqUrbOtro);
                        $("[name='EqUrbOtroNivel']").val(result.eqUrbOtroNivel);
                        $("[name='EqUrbOtroDistancia']").val(result.eqUrbOtroDistancia);
                        $("[name='ServUrbEducacionNivel']").val(result.servUrbEducacionNivel);
                        $("[name='ServUrbEducacionDistancia']").val(result.servUrbEducacionDistancia);
                        $("[name='ServUrbSaludNivel']").val(result.servUrbSaludNivel);
                        $("[name='ServUrbSaludDistancia']").val(result.servUrbSaludDistancia);
                        $("[name='ServUrbRecreacionNivel']").val(result.servUrbRecreacionNivel);
                        $("[name='ServUrbRecreacionDistancia']").val(result.servUrbRecreacionDistancia);
                        $("[name='ServUrbCulturaNivel']").val(result.servUrbCulturaNivel);
                        $("[name='ServUrbCulturaDistancia']").val(result.servUrbCulturaDistancia);
                        $("[name='ServUrbOtro']").val(result.servUrbOtro);
                        $("[name='ServUrbOtroNivel']").val(result.servUrbOtroNivel);
                        $("[name='ServUrbOtroDistancia']").val(result.servUrbOtroDistancia);
                        let topografia = result.cveTopografiaTerreno.split(",");
                        topografia.forEach(function (currentValue) {
                            $("[name='CveTopografiaTerreno'][value=" + currentValue + "]").prop("checked", true);
                        });
                        $("[name='EstratoArboreo']").val(result.estratoArboreo);
                        $("[name='EstratoArbustivo']").val(result.estratoArbustivo);
                        let hidrografia = result.cveHidrografia.split(",");
                        hidrografia.forEach(function (currentValue) {
                            $("[name='CveHidrografia'][value=" + currentValue + "]").prop("checked", true);
                        });
                        let restricciones = result.cveRestriccionesFederales.split(",");
                        restricciones.forEach(function (currentValue) {
                            $("[name='CveRestriccionesFederales'][value=" + currentValue + "]").prop("checked", true);
                        });
                        $("[name='DistanciaRios']").val(result.distanciaRios);
                        $("[name='DistanciaCorrientes']").val(result.distanciaCorrientes);
                        $("[name='DistanciaCuerposAgua']").val(result.distanciaCuerposAgua);
                        $("[name='DistanciaLineasAltaTension']").val(result.distanciaLineasAltaTension);
                        $("[name='DistanciaViasFerreas']").val(result.distanciaViasFerreas);
                        $("[name='DistanciaCarreteras']").val(result.distanciaCarreteras);
                        $("[name='DistanciaDuctos']").val(result.distanciaDuctos);
                        $("[name='DistanciaBarrancas']").val(result.distanciaBarrancas);
                        let caracteristicas = result.cveCaracteristicasSuelo.split(",");
                        caracteristicas.forEach(function (currentValue) {
                            $("[name='CveCaracteristicasSuelo'][value=" + currentValue + "]").prop("checked", true);
                        });
                        let otrasobs = result.cveOtrasObservaciones.split(",");
                        otrasobs.forEach(function (currentValue) {
                            $("[name='CveOtrasObservaciones'][value=" + currentValue + "]").prop("checked", true);
                        });
                        $("[name='PredioAptoVivienda'][value='" + result.predioAptoVivienda + "']").prop("checked", true);
                        $("[name='CondicionesPredioApto']").val(result.condicionesPredioApto);
                        $("[name='PorcentajeSuperficieUtillizable']").val(result.porcentajeSuperficieUtillizable);
                        $("[name='ObservacionesAdicionales']").val(result.observacionesAdicionales);
                        $("[name='NombreRealizoRecorrido']").val(result.nombreRealizoRecorrido);
                    } else {
                        $("#alert").html(result).fadeIn("slow");
                        showAlert();
                    }
                }
            });
        }
        function showAlert() {
            setTimeout(function () {
                $("#alert").slideToggle(1500);
            }, 4000);
            setTimeout(function () {
                $("#alert").remove();
            }, 6000);
        }
    </script>
}