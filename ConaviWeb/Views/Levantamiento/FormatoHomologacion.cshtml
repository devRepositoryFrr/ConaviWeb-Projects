@model ConaviWeb.Model.PrediosAdquisicion.Predio
@{
    ViewData["Title"] = "Formato de homologación";
}
<div id="predio">
    <form id="formInsertFormatoHomologacion" enctype="multipart/form-data" method="post" asp-controller="Levantamiento" asp-action="InsertFormatoLevantamiento">
        <input type="hidden" id="hd_id_predio" asp-for="Id" value="0" />
        <div class="row">
            <div class="col-md-12 center-block">
                <img src="../img/encab2021.png" class="img-responsive" />
            </div>
            @*<div class="col-md-7 text-right">
                <label>
                    SUBDIRECCIÓN GENERAL DE ADMINSTRACIÓN Y FINANCIAMIENTO<br />
                    COORDINACIÓN GENERAL DE ADMINISTRACIÓN<br />
                    DIRECCIÓN DE ADMINISTRACIÓN DE RECURSOS<br />
                    DEPARTAMENTO DE UNIDADES DOCUMENTALES
                </label>
            </div>*@
        </div>
        <br />
        <div class="row">
            <div class="col-md-12" id="alert">
                @Html.Raw(@ViewBag.Alert)
            </div>
        </div>
        <div class="row" style="background-color: #9D2449; color: white;">
            <div class="col-md-12 text-center">
                <label>
                    FORMATO DE HOMOLOGACIÓN DE IDENTIFICACIÓN DE ELEMENTOS EN EL ENTORNO A LOS PREDIOS DE ADQUISICIÓN
                </label>
            </div>
        </div>
        <div class="row form-group" style="background-color: #D4C19C;">
            <div class="col-md-12 text-center">
                <label>DATOS DEL PREDIO</label>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
                <label for="fecha_recorrido">Fecha de recorrido:</label>
            </div>
            <div class="col-md-5 datepicker-group">
                <input id="fecha_recorrido" class="form-control" asp-for="FechaRecorrido" />
                <span class="glyphicon glyphicon-calendar" aria-hidden="true"></span>
            </div>
        </div>
        <div class="row">
            <div class="col-md-3">
                <label>Domicilio:</label>
            </div>
            <div class="col-md-5">
                @*<input class="form-control" id="domicilio" asp-for="Domicilio" name="Domicilio" />*@
                <textarea class="form-control" id="domicilio" asp-for="Domicilio"></textarea>
            </div>
            <div class="col-md-4">
                <div class="row">
                    <div class="col-md-12 text-center">
                        <label>Coordenadas geográficas</label>
                    </div>
                    <div class="col-md-1">
                        <label>N</label>
                    </div>
                    <div class="col-md-5">
                        <input class="form-control" type="text" id="c_norte" asp-for="CoordN" />
                    </div>
                    <div class="col-md-1">
                        <label>O</label>
                    </div>
                    <div class="col-md-5">
                        <input class="form-control" type="text" id="c_oeste" asp-for="CoordO" />
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-4">
                <label>Estado:</label>
                @*<label class="form-control">Todas los estados</label>*@
                <select id="slctEstados" class="form-control" asp-for="CveEstado" asp-items="@ViewBag.EstadoCatalogo" data-val="true" data-val-required="Seleccione el estado" required onchange="getMunicipios(this.value)">
                    <option value="">--Seleccione--</option>
                </select>
            </div>
            <div class="col-md-4">
                <label>Municipio:</label>
                <select id="slctMunicipios" class="form-control" required onchange="getLocalidades(this.value)" asp-for="CveMunicipio">
                    <option value="">--Seleccione--</option>
                </select>
            </div>
            <div class="col-md-4">
                <label>Localidad:</label>
                <select id="slctLocalidades" class="form-control" asp-for="CveLocalidad" required>
                    <option value="">--Seleccione--</option>
                </select>
            </div>
        </div>
        <div class="row" style="background-color: #D4C19C;">
            <div class="col-md-12 text-center">
                <label>CARACTERÍSTICAS DEL PREDIO</label>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 text-center">
