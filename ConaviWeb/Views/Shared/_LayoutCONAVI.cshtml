    @*SpinnerConavi*@
    <link href="~/css/cssConavi/spinnerCnvi.css" rel="stylesheet" type="text/css" />
    @RenderSection("Scripts", required: false)
</head>
<body>
    <!-- Menú desplegable en vista parcial-->
