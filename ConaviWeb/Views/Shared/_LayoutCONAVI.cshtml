    <div class="container cnvi-body">
        <div class="red" style="text-align: right;">

            @if (User.Identity.IsAuthenticated)
            {
                <i class="fa fa-user-circle-o" aria-hidden="true"></i> <span>@User.Identity.Name</span>

                <a href="../LogOut">
                    <p style="font-size:20px;text-align:end; color:gray" class="ic-modulos enlace red-text"><i class="fa fa-sign-out"></i>Salir</p>
                </a>

            }
        </div>
        <ol class="breadcrumb">
            <li><a href="/Home"><i class="fa fa-home"> Inicio</i></a></li>
            <li class="active"><a id="breadcrumb_active" href=""></a></li>
        </ol>
        @RenderBody()
    </div>

    <br />
    <br />
    <br />
